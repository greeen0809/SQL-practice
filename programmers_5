# 업그레이드 할 수 없는 아이템 구하기
WITH PARENT AS (
    SELECT PARENT_ITEM_ID AS PARENT
    FROM ITEM_TREE
    GROUP BY PARENT_ITEM_ID
), JOIN_PARENT AS (
SELECT ITEM_ID,PARENT
FROM ITEM_TREE
LEFT JOIN PARENT ON ITEM_TREE.ITEM_ID = PARENT.PARENT
)
SELECT JOIN_PARENT.ITEM_ID
     , ITEM_INFO.ITEM_NAME
     , ITEM_INFO.RARITY
FROM JOIN_PARENT
LEFT JOIN ITEM_INFO ON JOIN_PARENT.ITEM_ID = ITEM_INFO.ITEM_ID
WHERE PARENT IS NULL
order by JOIN_PARENT.ITEM_ID desc
